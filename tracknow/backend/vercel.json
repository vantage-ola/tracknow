{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "crons": [
      {
        "path": "/api/v1/cron/update-f1-standings",
        "schedule": "45 15 * * 0"
      },
      {
        "path": "/api/v1/cron/update-f1-standings",
        "schedule": "0 0 * * *"
      },
      {
        "path": "/api/v1/cron/update-youtube-data",
        "schedule": "0 0 * * *"
      }
    ]
  }